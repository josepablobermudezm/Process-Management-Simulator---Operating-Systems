        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Proyecto22"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="1000" Loaded="Window_Loaded">
    <Grid Width="1000" Height="600">
        <Button Content="Back" HorizontalAlignment="Left" Margin="30,46,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_4"/>
