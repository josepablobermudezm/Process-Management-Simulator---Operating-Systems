            <TabItem Header="TabItem">
                <Grid Background="#FFE5E5E5">
                    <WebBrowser Name="myweb" HorizontalAlignment="Left" Height="394" Margin="0,10,0,0" VerticalAlignment="Top" Width="974"/>
                </Grid>
